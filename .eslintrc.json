{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "semi": "error",
        "max-len": [
            "error",
            {
                "code": 120
            }
        ],
        "no-unused-vars": [
            "warn"
        ]
    }
}